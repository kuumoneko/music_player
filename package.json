{
    "name": "music_player",
    "version": "3.0.0",
    "author": "Kuumoneko",
    "description": "Music player and download, support youtube and spotify",
    "private": true,
    "type": "module",
    "dependencies": {
        "@distube/ytdl-core": "^4.16.12",
        "express": "^5.1.0",
        "fluent-ffmpeg": "^2.1.3"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.27.1",
        "@fortawesome/free-brands-svg-icons": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "@fortawesome/react-fontawesome": "^0.2.2",
        "@hookform/resolvers": "^3.10.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.16",
        "@types/express": "^4.17.23",
        "@types/fluent-ffmpeg": "^2.1.27",
        "@types/node": "^16.18.126",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/parser": "^8.38.0",
        "@webpack-cli/generators": "^3.0.7",
        "cross-env": "^7.0.3",
        "nodemon": "^3.1.10",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.58.1",
        "react-refresh": "^0.14.2",
        "react-refresh-typescript": "^2.0.10",
        "react-refresh-webpack-plugin": "^0.1.0",
        "react-router-dom": "^6.30.1",
        "react-scripts": "^5.0.1",
        "tailwindcss": "^3.4.17",
        "ts-node": "^10.9.2",
        "webpack": "^5.99.9",
        "webpack-cli": "^5.1.4"
    },
    "scripts": {
        "start": "cross-env PORT=3001 GENERATE_SOURCEMAP=false SKIP_PREFLIGHT_CHECK=true react-scripts --openssl-legacy-provider start",
        "server": "nodemon --watch ./server --ext .ts --exec \"node --no-warnings=ExperimentalWarning --loader ts-node/esm server/index.ts\"",
        "build_frontend": "react-scripts build",
        "build_app": "node --no-warnings=ExperimentalWarning --loader ts-node/esm build.ts",
        "build_backend": "deno compile --no-check --import-map=deno.json --allow-sys --unstable-sloppy-imports --allow-env --allow-read --allow-net --allow-write --allow-run --output \"./app/server.exe\" ./server/index.ts",
        "test_new_feaures": "node --no-warnings=ExperimentalWarning --loader ts-node/esm test/music.ts"
    },
    "main": "src/index.ts",
    "homepage": "./src/",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 edge version"
        ]
    }
}
